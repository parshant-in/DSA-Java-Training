//Static variable
#code1 
class Student{
    static String collegeName = "IILM";
     String name;
    
    Student(String name){
        this.name = name;
    }
    void display(){
        System.out.println(name + " studied at"+ collegeName);
    }
}

public class Main
{
	public static void main(String[] args) {
	    Student s1 =new Student ("Parshant");
	    Student s2 =new Student ("yash");
	    s1.display();
	    s2.display();
	    
	}
}
