//Interface in java is abstract type that defines aset of methods a classs must implements
//Multipple inheritance 
#code 1
interface tv{
    void watchingTv();
}
interface food{
    void eatingFood();
}


class Student implements tv,food{
    public void watchingTv(){
         System.out.println("watching tv");
    }
    public void eatingFood(){
         System.out.println("eatingFood");
    
}
}

class Main {
    public static void main(String[] args) {
        
        Student s =new Student();
        s.watchingTv();
        s.eatingFood();
        
    }
}



#code 2
//Inheriting the constructor
// constructorcan be inherited using super keyword


class Person{
    Person(String name){
        System.out.println("Person Name:" + name);
    }
}

class CollegeStudent extends Person{
    CollegeStudent(String name){
        super(name);
        System.out.println("College Student constructor called");
        
    }
}


class Main {
     public static void main(String[] args){
         CollegeStudent std = new CollegeStudent("Parshant");
     }
}

#code 3
//inheriting attributes

class University{
     String UniversityName = "IILM University";
}

class College extends University{
    void displayUniversityName(){
        System.out.println("University:"+UniversityName);
    }
}

public class Main {
      public static void main(String[] args){
          College c = new College();
          c.displayUniversityName();
      }
}
