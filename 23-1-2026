 Simple Inheritance
#code 1
class Vehicle{
    void start(){
        System.out.println("Vehicle Start");
    }
}
class Bike extends Vehicle{
    void ride(){
       System.out.println("Bike running"); 
    }
}



public class Main
{
	public static void main(String[] args) {
	    Bike b = new Bike();
	    b.start();
	    b.ride();
	}
}

multiple Inheritance
#code 2
class Vehicle{
    void start(){
        System.out.println("Vehicle Start");
    }
}
    class Bike extends Vehicle{
    void ride(){
       System.out.println("Bike running"); 
    }
}

class speedometer extends Bike {
    void speed() {
        System.out.println("200km/hr");
    }
}

public class Main{
	public static void main(String[] args) {
	    speedometer s = new speedometer();
	    s.start();
	    s.ride();
	    s.speed();
	}
}
