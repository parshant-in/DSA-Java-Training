//Exception handling
//try and catch finally 
#code 1
public class Main
{
	public static void main(String[] args) {
	    
	    try {
	        
	        int balance =5000;
	    int withdraw=6000;
	     int result = balance-withdraw;
	        
	    } catch(Exception e) {
	        System.out.println("Error occured");
	    }finally{
	        System.out.println("transaction complete");
	    }
	   
	}
}

#code 2
//Throw keyword

class Bank{
    void withdraw(int amount){
        if(amount>5000){
            throw new ArithmeticException("insufficient balance");
    }
    }
}

public class Main
{
	public static void main(String[] args){
	    Bank b= new Bank();
	   b.withdraw(6000);
	}
}	
